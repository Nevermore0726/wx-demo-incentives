<!--pages/aboutUs/aboutUs.wxml-->
<view class="container">
  <image src="/images/myAccount/portrait.jpg" class="avatar"></image>
  <view class="section">
    <view class="login">
      <button class="login">登 录</button>
    </view>
    <view class="item" data-pay="wechat" bindtap="onItemClick">
      <image src="/images/myAccount/pay_wx_order.png" class="icon"></image>
      <text class="pay-text">支持</text>
    </view>
    <view class="item" data-pay="alipay" bindtap="onItemClick">
      <image src="/images/myAccount/pay_zhifubao_order.png" class="icon"></image>
      <text class="pay-text">支持</text>
    </view>
  </view>
</view>